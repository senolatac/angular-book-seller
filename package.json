{
  "name": "angular-book-seller",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config": "node set-env.js",
    "start": "node server.js",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider; ng build",
    "build-prod": "$(npm bin)/ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "heroku-postbuild": "npm run config && npm run build-prod"
